{
  "globalHeaders": {
    "Cross-Origin-Embedder-Policy": "unsafe-none"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/img/*",
      "/wasm/*",
      "/_next/*",
      "/*.{css,js,json,svg,png,jpg,jpeg,gif,ico,woff,woff2,ttf,eot,wasm}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "trailingSlash": "auto",
  "mimeTypes": {
    ".json": "application/json",
    ".wasm": "application/wasm"
  }
}
