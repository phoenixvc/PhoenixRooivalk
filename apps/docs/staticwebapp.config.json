{
  "globalHeaders": {
    "Cross-Origin-Opener-Policy": "same-origin-allow-popups",
    "Cross-Origin-Embedder-Policy": "unsafe-none"
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous", "authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/img/*", "/static/*", "/assets/*", "/*.{css,js,json,svg,png,jpg,jpeg,gif,ico,woff,woff2,ttf,eot}"]
  },
  "responseOverrides": {
    "401": {
      "rewrite": "/index.html"
    },
    "404": {
      "rewrite": "/404.html"
    }
  },
  "platform": {
    "apiRuntime": "node:20"
  },
  "trailingSlash": "auto",
  "mimeTypes": {
    ".json": "application/json",
    ".wasm": "application/wasm"
  }
}
