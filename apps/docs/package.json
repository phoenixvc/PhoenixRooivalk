{
  "name": "phoenixrooivalk-docs",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:strict": "DOCUSAURUS_BROKEN_LINKS=throw docusaurus build",
    "serve": "docusaurus serve",
    "lint": "prettier --check . && markdownlint \"**/*.md\" --ignore node_modules --disable MD025 MD051",
    "lint:fix": "markdownlint --fix '**/*.md' --ignore node_modules",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check-links": "../../scripts/check-docs-links.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/plugin-content-docs": "^3.9.2",
    "@docusaurus/plugin-content-pages": "^3.9.2",
    "@docusaurus/plugin-debug": "^3.9.2",
    "@docusaurus/plugin-google-analytics": "^3.9.1",
    "@docusaurus/plugin-google-gtag": "^3.9.1",
    "@docusaurus/plugin-ideal-image": "^3.9.2",
    "@docusaurus/plugin-pwa": "^3.9.1",
    "@docusaurus/plugin-sitemap": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.1",
    "@docusaurus/theme-classic": "^3.9.1",
    "@docusaurus/theme-mermaid": "^3.9.1",
    "@easyops-cn/docusaurus-search-local": "^0.45.0",
    "ajv": "^8.17.1",
    "clsx": "^2.1.0",
    "dompurify": "^3.2.4",
    "firebase": "^11.0.0",
