{
  "name": "phoenixrooivalk-docs",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:strict": "DOCUSAURUS_BROKEN_LINKS=throw docusaurus build",
    "serve": "docusaurus serve",
    "lint": "prettier --check . && markdownlint \"**/*.md\" --ignore node_modules --disable MD025 MD051",
