    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check-links": "../../scripts/check-docs-links.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
